<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/correctPop"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EB0B67C3">

    <Button
        android:id="@+id/fiveLetterGame"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginTop="100dp"
        android:gravity="center"
        android:onClick="fiveLetterStart"
        android:text="@string/fiveLetterStart"
        android:textSize="12sp" />

    <Button
        android:id="@+id/correctDone"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginBottom="100dp"
        android:gravity="center"
        android:onClick="doneNewGameClick"
        android:text="@string/correctDone"
        android:textSize="12sp" />


</RelativeLayout>